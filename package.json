{
  "name": "claytargettracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "DATABASE_URL=\"file:./dev.db\" next dev",
    "build": "DATABASE_URL=\"file:./dev.db\" next build",
    "start": "DATABASE_URL=\"file:./dev.db\" next start",
    "postinstall": "prisma generate",
    "lint": "eslint",
    "db:migrate": "DATABASE_URL=\"file:./dev.db\" npx prisma migrate dev",
    "db:reset": "DATABASE_URL=\"file:./dev.db\" npx prisma migrate reset",
    "db:studio": "DATABASE_URL=\"file:./dev.db\" npx prisma studio",
    "db:generate": "DATABASE_URL=\"file:./dev.db\" npx prisma generate",
    "demo:dev": "NEXT_PUBLIC_DEMO_MODE=true next dev",
    "build:demo": "mv app/api ./api-temp && NEXT_PUBLIC_DEMO_MODE=true next build && mv ./api-temp app/api || (mv ./api-temp app/api 2>/dev/null; exit 1)",
    "export:demo": "NEXT_PUBLIC_DEMO_MODE=true next build && next export"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.18.0",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "next": "16.0.0",
    "prisma": "^6.18.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.65.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "eslint": "^9",
    "eslint-config-next": "16.0.0"
  }
}
