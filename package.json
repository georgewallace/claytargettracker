{
  "name": "claytargettracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && npm run copy-prisma-binaries",
    "copy-prisma-binaries": "mkdir -p .next/server/node_modules/.prisma && cp -r node_modules/.prisma/client .next/server/node_modules/.prisma/ && mkdir -p .next/server/node_modules/@prisma && cp -r node_modules/@prisma/client .next/server/node_modules/@prisma/ || true",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "eslint",
    "db:push": "bash scripts/db-local-push.sh",
    "db:migrate:staging": "bash scripts/db-migrate-staging.sh",
    "db:create:migration": "bash scripts/db-create-migration.sh",
    "db:studio": "npx prisma studio",
    "db:generate": "npx prisma generate",
    "db:seed": "tsx scripts/seed-test-data.ts",
    "db:seed-history": "tsx scripts/seed-tournament-history.ts",
    "help:sync": "tsx scripts/sync-help-docs.ts",
    "demo:dev": "NEXT_PUBLIC_DEMO_MODE=true next dev",
    "build:demo": "mv app/api ./api-temp && NEXT_PUBLIC_DEMO_MODE=true next build && mv ./api-temp app/api || (mv ./api-temp app/api 2>/dev/null; exit 1)",
    "export:demo": "NEXT_PUBLIC_DEMO_MODE=true next build && next export"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.0",
    "@aws-sdk/client-s3": "^3.920.0",
    "@aws-sdk/client-ssm": "^3.916.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.18.0",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.6",
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "next": "16.0.0",
    "next-auth": "^5.0.0-beta.29",
    "prisma": "^6.18.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.65.0",
    "recharts": "^3.3.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "tsx": "^4.20.6"
  }
}
